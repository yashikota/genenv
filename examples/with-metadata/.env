# Example with metadata for field validation and descriptions

# @api_key [required] (string) API key for external service
API_KEY=abcdef123456789ghijklmno

# @api_secret [required] (string) API secret for external service
API_SECRET=pqrstuvwxyz0123456789ab

# @api_url [optional] (url) API endpoint URL
API_URL=https://api.example.com/v1

# @debug_mode [optional] (bool) Enable debug mode
DEBUG=true

# @retry_count [optional] (int) Number of retry attempts
RETRY_COUNT=3

# @timeout [optional] (float) Request timeout in seconds
TIMEOUT=5.5

# @admin_email [optional] (email) Admin notification email
ADMIN_EMAIL=admin@example.com

# @server_ip [optional] (ip) Server IP address
SERVER_IP=192.168.1.10

# Regular field without metadata
CACHE_DRIVER=file
