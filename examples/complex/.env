# Complex example with multiple features combined

# Application settings
# @app_name [optional] (string) Application name
APP_NAME=My Complex App
# @app_env [required] (string) Application environment (production, development, testing)
APP_ENV=development
# @app_key [required] (string) Application encryption key
APP_KEY=base64:abcdefghijklmnopqrstuvwxyz0123456789ABCDEF=
# @app_debug [optional] (bool) Enable debug mode
APP_DEBUG=true
# @app_url [required] (url) Application URL
APP_URL=https://myapp.example.com

# Database settings
# @db_connection [optional] (string) Database connection type
DB_CONNECTION=mysql
# @db_host [optional] (string) Database host
DB_HOST=127.0.0.1
# @db_port [optional] (int) Database port
DB_PORT=3306
# @db_database [required] (string) Database name
DB_DATABASE=myapp_db
# @db_username [required] (string) Database username
DB_USERNAME=db_user
# @db_password [required] (string) Database password
DB_PASSWORD=s3cur3p@ssw0rd!

# Redis settings
# @redis_host [optional] (string) Redis host
REDIS_HOST=127.0.0.1
# @redis_password [optional] (string) Redis password
REDIS_PASSWORD=null
# @redis_port [optional] (int) Redis port
REDIS_PORT=6379

# Mail settings
# @mail_driver [optional] (string) Mail driver
MAIL_DRIVER=smtp
# @mail_host [optional] (string) Mail host
MAIL_HOST=smtp.mailtrap.io
# @mail_port [optional] (int) Mail port
MAIL_PORT=2525
# @mail_username [optional] (string) Mail username
MAIL_USERNAME=mailtrap_user
# @mail_password [optional] (string) Mail password
MAIL_PASSWORD=mailtrap_password
# @mail_encryption [optional] (string) Mail encryption
MAIL_ENCRYPTION=tls
# @mail_from_address [optional] (email) Mail from address
MAIL_FROM_ADDRESS=noreply@example.com
# @mail_from_name [optional] (string) Mail from name
MAIL_FROM_NAME="${APP_NAME}"

# AWS settings
# @aws_access_key_id [optional] (string) AWS access key ID
AWS_ACCESS_KEY_ID=AKIAIOSFODNN7EXAMPLE
# @aws_secret_access_key [optional] (string) AWS secret access key
AWS_SECRET_ACCESS_KEY=wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY
# @aws_default_region [optional] (string) AWS default region
AWS_DEFAULT_REGION=us-west-2
# @aws_bucket [optional] (string) AWS bucket
AWS_BUCKET=my-app-bucket

# Pusher settings
# @pusher_app_id [optional] (string) Pusher app ID
PUSHER_APP_ID=12345
# @pusher_app_key [optional] (string) Pusher app key
PUSHER_APP_KEY=abcdef12345678
# @pusher_app_secret [optional] (string) Pusher app secret
PUSHER_APP_SECRET=fedcba87654321
# @pusher_app_cluster [optional] (string) Pusher app cluster
PUSHER_APP_CLUSTER=ap3
